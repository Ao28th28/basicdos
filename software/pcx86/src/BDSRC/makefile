ASM=MASM
LINK=LINK
EXE2BIN=EXE2BIN

.ASM.OBJ:
	$(ASM) /IINC $*;

.OBJ.EXE:
	$(LINK) $**,$@,$*/MAP;

.EXE.COM:
	$(EXE2BIN) $*.EXE $*.COM

BOOT.OBJ: BOOT.ASM INC\BIOS.INC INC\DISK.INC INC\MACROS.INC

BOOT.EXE: BOOT.OBJ

BOOT.COM: BOOT.EXE

WBOOT.OBJ: WBOOT.ASM INC\BIOS.INC INC\DISK.INC INC\MACROS.INC

WBOOT.EXE: WBOOT.OBJ

WBOOT.COM: WBOOT.EXE

IBMBIO.OBJ: IBMBIO.ASM INC\DRIVER.INC INC\BIOS.INC INC\DISK.INC INC\MACROS.INC

IBMBIO.EXE: IBMBIO.OBJ

IBMBIO.COM: IBMBIO.EXE

CONIO.OBJ: CONIO.ASM INC\DOS.INC INC\BIOS.INC INC\DISK.INC INC\MACROS.INC

DOSINT.OBJ: DOSINT.ASM INC\DOS.INC INC\BIOS.INC INC\DISK.INC INC\MACROS.INC

IBMDOS.OBJ: IBMDOS.ASM INC\DOS.INC INC\BIOS.INC INC\DISK.INC INC\MACROS.INC

IBMDOS.EXE: DOSINT.OBJ CONIO.OBJ IBMDOS.OBJ

IBMDOS.COM: IBMDOS.EXE
