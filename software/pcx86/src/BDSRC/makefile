ASM=MASM
LINK=LINK
EXE2BIN=EXE2BIN

.ASM.OBJ:
	$(ASM) /IINC $*;

.OBJ.EXE:
	$(LINK) $**,$@,$*/MAP;

.EXE.COM:
	$(EXE2BIN) $*.EXE $*.COM

BOOT.OBJ: BOOT.ASM BIOS.INC

BOOT.EXE: BOOT.OBJ

BOOT.COM: BOOT.EXE

WBOOT.OBJ: WBOOT.ASM

WBOOT.EXE: WBOOT.OBJ

WBOOT.COM: WBOOT.EXE

IBMBIO.OBJ: IBMBIO.ASM

IBMBIO.EXE: IBMBIO.OBJ

IBMBIO.COM: IBMBIO.EXE
