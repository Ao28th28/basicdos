ASM=MASM
LINK=LINK
EXE2BIN=EXE2BIN

.ASM.OBJ:
	$(ASM) /IINC $**,$@;

.ASM.LST:
	$(ASM) /IINC $**,$@,$*.LST;

.OBJ.EXE:
	$(LINK) $**,$@;

.EXE.COM:
	$(EXE2BIN) $** $@

BOOT\BOOT.OBJ: BOOT\BOOT.ASM

BOOT\BOOT.EXE: BOOT\BOOT.OBJ
	$(LINK) $**,$@,BOOT\$*.MAP/MAP;

BOOT\BOOT.COM: BOOT\BOOT.EXE

BOOT\WBOOT.OBJ: BOOT\WBOOT.ASM

BOOT\WBOOT.EXE: BOOT\WBOOT.OBJ
	$(LINK) $**,$@,BOOT\$*.MAP/MAP;

BOOT\WBOOT.COM: BOOT\WBOOT.EXE
	$(EXE2BIN) $** $@
	BOOT\WBOOT.COM BOOT\BOOT.COM

DEV\NULDEV.OBJ: DEV\NULDEV.ASM

DEV\NULDEV.EXE: DEV\NULDEV.OBJ

DEV\NULDEV.COM: DEV\NULDEV.EXE

DEV\KBDDEV.OBJ: DEV\KBDDEV.ASM

DEV\KBDDEV.EXE: DEV\KBDDEV.OBJ

DEV\KBDDEV.COM: DEV\KBDDEV.EXE

DEV\SCRDEV.OBJ: DEV\SCRDEV.ASM

DEV\SCRDEV.EXE: DEV\SCRDEV.OBJ

DEV\SCRDEV.COM: DEV\SCRDEV.EXE

DEV\CONDEV.OBJ: DEV\CONDEV.ASM

DEV\CONDEV.EXE: DEV\CONDEV.OBJ

DEV\CONDEV.COM: DEV\CONDEV.EXE

DEV\AUXDEV.OBJ: DEV\AUXDEV.ASM

DEV\AUXDEV.EXE: DEV\AUXDEV.OBJ

DEV\AUXDEV.COM: DEV\AUXDEV.EXE

DEV\COMDEV.OBJ: DEV\COMDEV.ASM

DEV\COMDEV.EXE: DEV\COMDEV.OBJ

DEV\COMDEV.COM: DEV\COMDEV.EXE

DEV\PRNDEV.OBJ: DEV\PRNDEV.ASM

DEV\PRNDEV.EXE: DEV\PRNDEV.OBJ

DEV\PRNDEV.COM: DEV\PRNDEV.EXE

DEV\LPTDEV.OBJ: DEV\LPTDEV.ASM

DEV\LPTDEV.EXE: DEV\LPTDEV.OBJ

DEV\LPTDEV.COM: DEV\LPTDEV.EXE

DEV\CLOCKDEV.OBJ: DEV\CLOCKDEV.ASM

DEV\CLOCKDEV.EXE: DEV\CLOCKDEV.OBJ

DEV\CLOCKDEV.COM: DEV\CLOCKDEV.EXE

DEV\DISKDEV.OBJ: DEV\DISKDEV.ASM

DEV\DISKDEV.EXE: DEV\DISKDEV.OBJ

DEV\DISKDEV.COM: DEV\DISKDEV.EXE

DEV\IBMBIO.OBJ: DEV\IBMBIO.ASM

DEV\IBMBIO.EXE: DEV\IBMBIO.OBJ
	$(LINK) @DEV\IBMBIO.LRF

DEV\IBMBIO.TMP: DEV\IBMBIO.EXE
	$(EXE2BIN) $** $@
	COPY /B BOOT\BOOT2.COM+DEV\NULDEV.COM+DEV\KBDDEV.COM+DEV\SCRDEV.COM+DEV\CONDEV.COM+DEV\AUXDEV.COM DEV\IBMDEV.TMP
	COPY /B DEV\IBMDEV.TMP+DEV\PRNDEV.COM+DEV\LPTDEV.COM+DEV\CLOCKDEV.COM+DEV\DISKDEV.COM+DEV\IBMBIO.TMP DEV\IBMBIO.COM
	DEL DEV\IBMDEV.TMP

DOS\CONIO.OBJ: DOS\CONIO.ASM

DOS\DOSINT.OBJ: DOS\DOSINT.ASM

DOS\IBMDOS.OBJ: DOS\IBMDOS.ASM

DOS\IBMDOS.EXE: DOS\DOSINT.OBJ DOS\CONIO.OBJ DOS\IBMDOS.OBJ
	$(LINK) @DOS\IBMDOS.LRF

DOS\IBMDOS.COM: DOS\IBMDOS.EXE
